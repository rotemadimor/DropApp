<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.schoolproject1">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/final_logo2"
        android:label="@string/appname"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".CurrentSystem"></activity>
        <activity android:name=".FamilyUsers"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".MyService" android:exported="false"></service>
    </application>

</manifest>